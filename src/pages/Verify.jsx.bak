import React, { useState, useEffect } from "react";
import {
  Shield,
  CheckCircle,
  AlertCircle,
  ExternalLink,
  FileText,
  User,
  Hash,
  Search,
  Loader,
  ArrowLeft,
  Copy,
  Check,
  Download,
  GraduationCap,
} from "lucide-react";
import apiService from "../services/api";

const Verify = () => {
  const [verificationData, setVerificationData] = useState(null);
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState("");
  const [searchQuery, setSearchQuery] = useState("");
  const [copied, setCopied] = useState(false);

  useEffect(() => {
    // Check URL parameters for auto-verification
    const urlParams = new URLSearchParams(window.location.search);
    const docId = urlParams.get("doc");
    const hash = urlParams.get("hash");

    if (docId || hash) {
      handleVerification(docId || hash);
    }
  }, []);

  const handleVerification = async (query) => {
    if (!query) {
      setError("Please enter a document ID or hash");
      return;
    }

    setLoading(true);
    setError("");

    try {
      // Call the verification API using our API service
      const data = await apiService.verifyDocument(query);

      if (data.valid) {
        setVerificationData(data);
      } else {
        setError("Document not found or invalid");
      }
    } catch (err) {
      console.error("Verification failed:", err);
      setError("Failed to verify document. Please try again.");
    } finally {
      setLoading(false);
    }
  };

  const handleSubmit = (e) => {
    e.preventDefault();
    handleVerification(searchQuery);
  };

  const copyToClipboard = (text) => {
    navigator.clipboard.writeText(text);
    setCopied(true);
    setTimeout(() => setCopied(false), 2000);
  };

  return (
    <div className="min-h-screen bg-[#EFECE3] p-4 sm:p-6 lg:p-8">
      <div className="max-w-4xl mx-auto">
        {/* Mobile-First Header */}
        <div className="text-center mb-6 sm:mb-8">
          <div className="inline-flex items-center justify-center w-16 h-16 sm:w-20 sm:h-20 bg-[#4A70A9] rounded-2xl mb-4 shadow-lg">
            <Shield size={32} className="text-white sm:w-10 sm:h-10" />
          </div>
          <h1 className="text-2xl sm:text-3xl lg:text-4xl font-bold text-[#4A70A9] mb-2">
            Document Verification
          </h1>
          <p className="text-[#4A70A9] text-sm sm:text-base">
            Verify blockchain-secured documents instantly
          </p>
        </div>

        {/* Mobile-First Search Form */}
        {Object.keys(verificationData || {}).length === 0 && (
          <div className="bg-white border border-[#8FABD4]/40 rounded-2xl p-4 sm:p-6 lg:p-8 mb-6 sm:mb-8 shadow-sm">
            <form onSubmit={handleSubmit} className="space-y-4">
              <div>
                <label className="block text-sm font-medium text-[#4A70A9] mb-2">
                  Document ID or Hash
                </label>
                <div className="flex flex-col sm:flex-row gap-3 sm:gap-4">
                  <input
                    type="text"
                    value={searchQuery}
                    onChange={(e) => setSearchQuery(e.target.value)}
                    className="flex-1 bg-[#EFECE3] border-2 border-[#8FABD4]/60 rounded-xl px-4 py-3 text-[#000000] placeholder-[#8FABD4] focus:ring-2 focus:ring-[#4A70A9]/40 focus:border-[#4A70A9]/60 outline-none text-sm sm:text-base"
                    placeholder="Enter document ID or verification hash..."
                  />
                  <button
                    type="submit"
                    disabled={loading || !searchQuery.trim()}
                    className="w-full sm:w-auto bg-[#4A70A9] hover:bg-[#3A5A89] disabled:opacity-50 disabled:cursor-not-allowed text-white px-6 py-3 rounded-xl flex items-center justify-center space-x-2 transition-colors font-medium"
                  >
                    {loading ? (
                      <>
                        <Loader size={20} className="animate-spin" />
                        <span>Verifying...</span>
                      </>
                    ) : (
                      <>
                        <Search size={20} />
                        <span>Verify</span>
                      </>
                    )}
                  </button>
                </div>
              </div>
            </form>
          </div>
        )}

        {/* Error Display */}
        {error && (
          <div className="bg-red-50 border border-red-200 rounded-2xl p-4 mb-6 sm:mb-8">
            <div className="flex items-start space-x-3">
              <AlertCircle
                size={20}
                className="text-red-600 flex-shrink-0 mt-0.5"
              />
              <p className="text-red-700 text-sm sm:text-base">{error}</p>
            </div>
          </div>
        )}

        {/* Verification Results */}
        {verificationData && (
          <div className="space-y-4 sm:space-y-6">
            {/* Status Header - Mobile First */}
            <div
              className={`border rounded-2xl p-4 sm:p-6 ${
                verificationData.valid
                  ? "bg-green-50 border-green-200"
                  : "bg-red-50 border-red-200"
              }`}
            >
              <div className="flex flex-col sm:flex-row sm:items-center space-y-3 sm:space-y-0 sm:space-x-4">
                <div className="flex items-center space-x-3">
                  {verificationData.valid ? (
                    <CheckCircle
                      size={28}
                      className="text-green-400 flex-shrink-0"
                    />
                  ) : (
                    <AlertCircle
                      size={28}
                      className="text-red-400 flex-shrink-0"
                    />
                  )}
                  <div>
                    <h2
                      className={`text-lg sm:text-xl lg:text-2xl font-bold ${
                        verificationData.valid
                          ? "text-green-800"
                          : "text-red-800"
                      }`}
                    >
                      {verificationData.valid
                        ? "✓ Valid Verification Code"
                        : "✗ Verification Failed"}
                    </h2>
                    <p
                      className={`text-sm sm:text-base ${
                        verificationData.valid
                          ? "text-green-700"
                          : "text-red-700"
                      }`}
                    >
                      {verificationData.valid
                        ? "The following details are confirmed by verify.demo-university.com"
                        : "This document could not be verified"}
                    </p>
                  </div>
                </div>
              </div>
            </div>

            {/* Certificate-Style Card */}
            {verificationData.valid && (
              <div className="bg-white rounded-2xl shadow-2xl overflow-hidden">
                {/* Green Validation Header */}
                <div className="bg-gradient-to-r from-[#4A70A9] to-[#3A5A89] p-4 sm:p-6 text-white">
                  <div className="flex items-center justify-center space-x-2">
                    <CheckCircle size={24} className="flex-shrink-0" />
                    <h2 className="text-xl sm:text-2xl font-bold">
                      Valid Verification Code
                    </h2>
                  </div>
                </div>

                {/* University Logo and Name */}
                <div className="p-6 sm:p-8 border-b border-gray-200">
                  <div className="flex flex-col items-center text-center space-y-4">
                    {verificationData.university?.logo ? (
                      <img
                        src={verificationData.university.logo}
                        alt={verificationData.university.name || "University"}
                        className="w-20 h-20 sm:w-24 sm:h-24 object-contain"
                      />
                    ) : (
                      <div className="w-20 h-20 sm:w-24 sm:h-24 bg-gradient-to-br from-[#4A70A9] to-[#3A5A89] rounded-full flex items-center justify-center">
                        <GraduationCap size={40} className="text-white" />
                      </div>
                    )}
                    <div>
                      <h1 className="text-2xl sm:text-3xl font-bold text-gray-800">
                        {verificationData.university?.name ||
                          verificationData.document?.issuer ||
                          "Demo University"}
                      </h1>
                      <p className="text-sm text-gray-500 mt-1">
                        The following details are confirmed by:{" "}
                        {verificationData.university?.name
                          ? `verify.${verificationData.university.name
                              .toLowerCase()
                              .replace(/\s+/g, "-")}.com`
                          : "verify.demo-university.com"}
                      </p>
                    </div>
                  </div>
                </div>

                {/* Certificate Details Grid */}
                <div className="p-6 sm:p-8 bg-gray-50">
                  <div className="grid grid-cols-1 sm:grid-cols-2 gap-4 sm:gap-6">
                    {/* Student Name */}
                    <div className="bg-white rounded-xl p-4 shadow-sm">
                      <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                        Name
                      </p>
                      <p className="text-lg font-bold text-gray-800">
                        {verificationData.holder?.name ||
                          verificationData.document?.studentName ||
                          "N/A"}
                      </p>
                    </div>

                    {/* Student Number */}
                    {verificationData.holder?.studentNumber && (
                      <div className="bg-white rounded-xl p-4 shadow-sm">
                        <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                          Student Number
                        </p>
                        <p className="text-lg font-bold text-gray-800">
                          {verificationData.holder.studentNumber}
                        </p>
                      </div>
                    )}

                    {/* Degree/Course */}
                    {verificationData.document?.degree && (
                      <div className="bg-white rounded-xl p-4 shadow-sm">
                        <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                          Degree
                        </p>
                        <p className="text-lg font-bold text-gray-800">
                          {verificationData.document.degree}
                        </p>
                      </div>
                    )}

                    {/* Batch Number */}
                    {verificationData.batch?.batchId && (
                      <div className="bg-white rounded-xl p-4 shadow-sm">
                        <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                          Batch Number
                        </p>
                        <p className="text-base font-mono text-gray-800 break-all">
                          {verificationData.batch.batchId}
                        </p>
                      </div>
                    )}

                    {/* Document Code */}
                    <div className="bg-white rounded-xl p-4 shadow-sm sm:col-span-2">
                      <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                        Document Code / Serial Number
                      </p>
                      <p className="text-base font-mono text-gray-800 break-all">
                        {verificationData.docId}
                      </p>
                    </div>

                    {/* Grade */}
                    {verificationData.document?.grade && (
                      <div className="bg-white rounded-xl p-4 shadow-sm">
                        <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                          Grade
                        </p>
                        <p className="text-lg font-bold text-gray-800">
                          {verificationData.document.grade}
                        </p>
                      </div>
                    )}

                    {/* Date of Graduation / Issue Date */}
                    {verificationData.document?.issuedAt && (
                      <div className="bg-white rounded-xl p-4 shadow-sm">
                        <p className="text-xs font-semibold text-gray-500 uppercase tracking-wide mb-1">
                          Date of Graduation
                        </p>
                        <p className="text-lg font-bold text-gray-800">
                          {new Date(
                            verificationData.document.issuedAt
                          ).toLocaleDateString("en-US", {
                            day: "numeric",
                            month: "long",
                            year: "numeric",
                          })}
                        </p>
                      </div>
                    )}
                  </div>
                </div>

                {/* Viewable Files Section */}
                {verificationData.access && (
                  <div className="p-6 sm:p-8 bg-white border-t border-gray-200">
                    <h3 className="text-lg font-bold text-gray-800 mb-4 flex items-center space-x-2">
                      <FileText size={20} className="text-[#4A70A9]" />
                      <span>Viewable File(s)</span>
                    </h3>
                    <div className="space-y-3">
                      {verificationData.access.displayDocument &&
                        verificationData.access.displayDocument !== "null" && (
                          <a
                            href={verificationData.access.displayDocument}
                            target="_blank"
                            rel="noopener noreferrer"
                            className="flex items-center justify-between bg-[#EFECE3] hover:bg-white border border-[#8FABD4]/60 rounded-xl p-4 transition-colors group"
                          >
                            <div className="flex items-center space-x-3">
                              <div className="w-10 h-10 bg-[#4A70A9] rounded-lg flex items-center justify-center flex-shrink-0">
                                <FileText size={20} className="text-white" />
                              </div>
                              <div>
                                <p className="font-semibold text-gray-800 group-hover:text-[#3A5A89]">
                                  {verificationData.document?.title ||
                                    `${
                                      verificationData.holder?.name ||
                                      "Document"
                                    }.pdf`}
                                </p>
                                <p className="text-xs text-gray-500">
                                  {verificationData.document?.title
                                    ? `${Math.floor(
                                        Math.random() * 200 + 50
                                      )} KB`
                                    : "259 KB"}
                                </p>
                              </div>
                            </div>
                            <ExternalLink
                              size={20}
                              className="text-gray-400 group-hover:text-[#4A70A9]"
                            />
                          </a>
                        )}

                      {verificationData.access.canonicalDocument && (
                        <a
                          href={verificationData.access.canonicalDocument}
                          target="_blank"
                          rel="noopener noreferrer"
                          className="flex items-center justify-between bg-[#EFECE3] hover:bg-white border border-[#8FABD4]/60 rounded-xl p-4 transition-colors group"
                        >
                          <div className="flex items-center space-x-3">
                            <div className="w-10 h-10 bg-[#4A70A9] rounded-lg flex items-center justify-center flex-shrink-0">
                              <Download size={20} className="text-white" />
                            </div>
                            <div>
                              <p className="font-semibold text-gray-800 group-hover:text-[#3A5A89]">
                                Download Original Document
                              </p>
                              <p className="text-xs text-gray-500">
                                Canonical version (IPFS)
                              </p>
                            </div>
                          </div>
                          <ExternalLink
                            size={20}
                            className="text-gray-400 group-hover:text-[#4A70A9]"
                          />
                        </a>
                      )}
                    </div>
                  </div>
                )}
              </div>
            )}

            {/* Merkle Hashes - Mobile Responsive */}
            {verificationData.merkleHashes && (
              <div className="bg-white border border-[#8FABD4]/50 rounded-2xl p-4 sm:p-6 shadow-sm">
                <h3 className="text-lg sm:text-xl font-bold text-[#4A70A9] mb-4 flex items-center space-x-2">
                  <Hash size={20} className="text-[#4A70A9]" />
                  <span>Merkle Hashes</span>
                </h3>

                <div className="space-y-3 sm:space-y-4">
                  {verificationData.merkleHashes.personal && (
                    <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                      <div className="flex items-center justify-between mb-1">
                        <p className="text-xs sm:text-sm text-[#4A70A9]">
                          Personal Merkle Hash
                        </p>
                        <button
                          onClick={() =>
                            copyToClipboard(
                              verificationData.merkleHashes.personal
                            )
                          }
                          className="p-1 hover:bg-white rounded transition-colors"
                        >
                          {copied ? (
                            <Check size={14} className="text-green-600" />
                          ) : (
                            <Copy size={14} className="text-[#4A70A9]" />
                          )}
                        </button>
                      </div>
                      <p className="text-[#000000] font-mono text-xs sm:text-sm break-all">
                        {verificationData.merkleHashes.personal}
                      </p>
                    </div>
                  )}

                  {verificationData.merkleHashes.root && (
                    <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                      <div className="flex items-center justify-between mb-1">
                        <p className="text-xs sm:text-sm text-[#4A70A9]">
                          Root Merkle Hash
                        </p>
                        <button
                          onClick={() =>
                            copyToClipboard(verificationData.merkleHashes.root)
                          }
                          className="p-1 hover:bg-white rounded transition-colors"
                        >
                          {copied ? (
                            <Check size={14} className="text-green-600" />
                          ) : (
                            <Copy size={14} className="text-[#4A70A9]" />
                          )}
                        </button>
                      </div>
                      <p className="text-[#000000] font-mono text-xs sm:text-sm break-all">
                        {verificationData.merkleHashes.root}
                      </p>
                    </div>
                  )}
                </div>
              </div>
            )}

            {/* Blockchain Information - Mobile Responsive */}
            {verificationData.verification && (
              <div className="bg-white border border-[#8FABD4]/50 rounded-2xl p-4 sm:p-6 shadow-sm">
                <h3 className="text-lg sm:text-xl font-bold text-[#4A70A9] mb-4 flex items-center space-x-2">
                  <Hash size={20} className="text-[#4A70A9]" />
                  <span>Blockchain Information</span>
                </h3>

                <div className="space-y-3 sm:space-y-4">
                  {verificationData.verification.blockchain_network && (
                    <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                      <p className="text-xs sm:text-sm text-[#4A70A9] mb-1">
                        Network
                      </p>
                      <p className="text-[#000000] text-sm sm:text-base font-medium">
                        {verificationData.verification.blockchain_network}
                      </p>
                    </div>
                  )}

                  {verificationData.verification.transaction_signature && (
                    <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                      <div className="flex items-center justify-between mb-1">
                        <p className="text-xs sm:text-sm text-[#4A70A9]">
                          Transaction Signature
                        </p>
                        <button
                          onClick={() =>
                            copyToClipboard(
                              verificationData.verification
                                .transaction_signature
                            )
                          }
                          className="p-1 hover:bg-white rounded transition-colors"
                        >
                          {copied ? (
                            <Check size={14} className="text-green-600" />
                          ) : (
                            <Copy size={14} className="text-[#4A70A9]" />
                          )}
                        </button>
                      </div>
                      <p className="text-[#000000] font-mono text-xs sm:text-sm break-all">
                        {verificationData.verification.transaction_signature}
                      </p>
                    </div>
                  )}

                  {verificationData.verification.explorer_url && (
                    <div className="pt-2">
                      <a
                        href={verificationData.verification.explorer_url}
                        target="_blank"
                        rel="noopener noreferrer"
                        className="inline-flex items-center space-x-2 bg-[#4A70A9]/10 hover:bg-[#4A70A9]/20 border border-[#4A70A9]/40 rounded-xl px-4 py-3 text-[#3A5A89] transition-colors text-sm sm:text-base font-medium w-full sm:w-auto justify-center sm:justify-start"
                      >
                        <ExternalLink size={16} />
                        <span>View on Blockchain Explorer</span>
                      </a>
                    </div>
                  )}
                </div>
              </div>
            )}

            {/* Verification Steps - Mobile Responsive */}
            {verificationData.verification?.steps && (
              <div className="bg-white border border-[#8FABD4]/50 rounded-2xl p-4 sm:p-6 shadow-sm">
                <h3 className="text-lg sm:text-xl font-bold text-[#4A70A9] mb-4 flex items-center space-x-2">
                  <Shield size={20} className="text-[#4A70A9]" />
                  <span>Verification Process</span>
                </h3>

                <div className="space-y-3 sm:space-y-4">
                  {verificationData.verification.steps.map((step, index) => (
                    <div
                      key={index}
                      className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40"
                    >
                      <div className="flex items-start space-x-3">
                        <div
                          className={`w-8 h-8 rounded-full flex items-center justify-center text-sm font-bold flex-shrink-0 ${
                            step.status === "valid"
                              ? "bg-green-500 text-white"
                              : "bg-red-500 text-white"
                          }`}
                        >
                          {step.step}
                        </div>
                        <div className="flex-1 min-w-0">
                          <div className="flex items-start justify-between mb-1">
                            <h4 className="text-[#000000] font-semibold text-sm sm:text-base">
                              {step.name}
                            </h4>
                            {step.status === "valid" ? (
                              <CheckCircle
                                size={16}
                                className="text-green-400 flex-shrink-0 ml-2"
                              />
                            ) : (
                              <AlertCircle
                                size={16}
                                className="text-red-400 flex-shrink-0 ml-2"
                              />
                            )}
                          </div>
                          <p
                            className={`text-xs sm:text-sm ${
                              step.status === "valid"
                                ? "text-green-700"
                                : "text-red-700"
                            }`}
                          >
                            {step.details?.message ||
                              (step.status === "valid"
                                ? "Verification successful"
                                : "Verification failed")}
                          </p>

                          {/* Step Details */}
                          {step.details && (
                            <div className="mt-2 space-y-2">
                              {step.details.merkleRoot && (
                                <div className="text-xs">
                                  <p className="text-[#4A70A9]">Merkle Root:</p>
                                  <p className="text-[#000000] font-mono break-all">
                                    {step.details.merkleRoot}
                                  </p>
                                </div>
                              )}
                              {step.details.explorerUrl && (
                                <a
                                  href={step.details.explorerUrl}
                                  target="_blank"
                                  rel="noopener noreferrer"
                                  className="inline-flex items-center space-x-1 text-[#4A70A9] hover:text-[#3A5A89] text-xs"
                                >
                                  <ExternalLink size={12} />
                                  <span>View Details</span>
                                </a>
                              )}
                            </div>
                          )}
                        </div>
                      </div>
                    </div>
                  ))}
                </div>
              </div>
            )}

            {/* Summary - Mobile Responsive */}
            {verificationData.verification?.summary && (
              <div className="bg-white border border-[#8FABD4]/50 rounded-2xl p-4 sm:p-6 shadow-sm">
                <h3 className="text-lg sm:text-xl font-bold text-[#4A70A9] mb-4">
                  Verification Summary
                </h3>
                <div className="grid grid-cols-1 sm:grid-cols-2 gap-3 sm:gap-4">
                  {[
                    { key: "identityHashValid", label: "Identity Hash" },
                    { key: "merkleProofValid", label: "Merkle Proof" },
                    { key: "blockchainValid", label: "Blockchain" },
                    { key: "overallValid", label: "Overall Status" },
                  ].map(({ key, label }) => (
                    <div
                      key={key}
                      className="flex items-center justify-between bg-[#EFECE3] rounded-xl p-3 border border-[#8FABD4]/40"
                    >
                      <span className="text-[#4A70A9] text-sm">{label}:</span>
                      <div className="flex items-center space-x-2">
                        {verificationData.verification.summary[key] ? (
                          <CheckCircle size={16} className="text-green-400" />
                        ) : (
                          <AlertCircle size={16} className="text-red-400" />
                        )}
                        <span
                          className={`text-sm font-medium ${
                            verificationData.verification.summary[key]
                              ? "text-green-700"
                              : "text-red-700"
                          }`}
                        >
                          {verificationData.verification.summary[key]
                            ? "Valid"
                            : "Invalid"}
                        </span>
                      </div>
                    </div>
                  ))}
                </div>
              </div>
            )}

            {/* Document Access - Mobile Responsive */}
            {verificationData.access && (
              <div className="bg-white border border-[#8FABD4]/50 rounded-2xl p-4 sm:p-6 shadow-sm">
                <h3 className="text-lg sm:text-xl font-bold text-[#4A70A9] mb-4">
                  Document Access
                </h3>
                <div className="space-y-3">
                  {verificationData.access.canonicalDocument && (
                    <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                      <p className="text-xs sm:text-sm text-[#4A70A9] mb-2">
                        Canonical Document (IPFS)
                      </p>
                      <a
                        href={verificationData.access.canonicalDocument}
                        target="_blank"
                        rel="noopener noreferrer"
                        className="text-[#4A70A9] hover:text-[#3A5A89] text-xs sm:text-sm break-all"
                      >
                        {verificationData.access.canonicalDocument}
                      </a>
                    </div>
                  )}
                  {verificationData.access.displayDocument &&
                    verificationData.access.displayDocument !== "null" && (
                      <div className="bg-[#EFECE3] rounded-xl p-3 sm:p-4 border border-[#8FABD4]/40">
                        <p className="text-xs sm:text-sm text-[#4A70A9] mb-2">
                          Display Document
                        </p>
                        <a
                          href={verificationData.access.displayDocument}
                          target="_blank"
                          rel="noopener noreferrer"
                          className="text-[#4A70A9] hover:text-[#3A5A89] text-xs sm:text-sm break-all"
                        >
                          {verificationData.access.displayDocument}
                        </a>
                      </div>
                    )}
                </div>
              </div>
            )}
          </div>
        )}

        {/* Footer */}
        <div className="text-center mt-8 sm:mt-12 py-4">
          <div className="inline-flex items-center justify-center space-x-2 bg-white/80 backdrop-blur-sm rounded-full px-5 py-2.5 shadow-sm border border-[#8FABD4]/30 mb-2">
            <Shield size={16} className="text-[#4A70A9]" />
            <span className="text-sm font-medium text-[#4A70A9]">
              Powered by:
            </span>
            <a
              href="https://xertiq.com"
              target="_blank"
              rel="noopener noreferrer"
              className="text-sm font-bold text-[#3A5A89] hover:text-[#4A70A9] transition-colors"
            >
              XertiQ
            </a>
          </div>
          <p className="text-xs text-[#8FABD4]">
            Blockchain-Secured Document Verification
          </p>
        </div>
      </div>
    </div>
  );
};

export default Verify;
